{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es6"],
    "types": ["@types/chrome", "node"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "noEmit": true,
    "jsx": "react"
  },
  "include": ["src"]
}
